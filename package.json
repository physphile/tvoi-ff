{
	"name": "template-spa-react",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"stylelint": "stylelint \"src/**/*.css\"",
		"typecheck": "tsc --noEmit",
		"biome": "biome check ./src",
		"fmt": "bun biome --write && bun stylelint --fix",
		"knip": "knip",
		"dpdm": "dpdm --exit-code circular:1 --no-tree --no-warning --progress false --transform './src/app/main.tsx'",
		"sb": "storybook dev -p 6006",
		"sb:build": "storybook build",
		"ct": "playwright test -c playwright-ct.config.ts",
		"openapi-ts": "openapi-ts --input ",
		"e2e": "playwright test --ui"
	},
	"dependencies": {
		"@gravity-ui/date-utils": "^2.5.6",
		"@gravity-ui/navigation": "^3.1.4",
		"@gravity-ui/table": "^1.9.0",
		"@hey-api/client-fetch": "^0.8.3",
		"@tanstack/react-query": "^5.67.2",
		"ahooks": "^3.8.4",
		"konva": "^9.3.18",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-error-boundary": "^5.0.0",
		"react-hook-form": "^7.54.2",
		"react-konva": "^19.0.3",
		"react-router": "^7.3.0",
		"use-image": "^1.1.1",
		"usehooks-ts": "^3.1.1"
	},
	"devDependencies": {
		"@bem-react/classname": "1.6.0",
