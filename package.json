		"knip": "^5.45.0",
		"sass-embedded": "^1.85.1",
		"storybook": "^8.6.4",
		"storybook-dark-mode": "^4.0.2",
		"stylelint": "^16.15.0",
		"stylelint-config-recommended": "^15.0.0",
		"stylelint-order": "^6.0.4",
		"typescript": "^5.8.2",
		"vite": "^6.2.1",
		"vite-plugin-mkcert": "^1.17.7"
	},
	"module": "index.ts"
